{
    "name": "swarrot/swarrot-bundle",
    "description": "SwarrotBundle",
    "type": "symfony-bundle",
    "keywords": ["swarrot","bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dolbeau",
            "homepage": "http://odolbeau.fr/"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/log": "^1.0",
        "swarrot/swarrot": "^3.4",
        "symfony/config": "^3.4|^4.1",
        "symfony/console": "^3.4|^4.1",
        "symfony/dependency-injection": "^3.4|^4.1",
        "symfony/http-kernel": "^3.4|^4.1",
        "symfony/yaml": "^3.4|^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/phpunit-bridge": "^4.0",
        "doctrine/dbal": "^2.2",
        "doctrine/common": "^2.2",
        "sentry/sentry-symfony": "^2.3",
        "php-amqplib/php-amqplib": "^2.9"
    },
    "suggest": {
        "php-amqplib/php-amqplib": "Required if using amqp_lib"
    },
    "autoload": {
        "psr-4": { "Swarrot\\SwarrotBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7-dev"
        }
    }
}
